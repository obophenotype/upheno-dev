prefix IAO: <http://purl.obolibrary.org/obo/IAO_>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix oio: <http://www.geneontology.org/formats/oboInOwl#>
prefix def: <http://purl.obolibrary.org/obo/IAO_0000115>
prefix owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT distinct ?phenotype ?trait

WHERE 
{
  ?phenotype a owl:Class;
          rdfs:subClassOf+ <http://purl.obolibrary.org/obo/UPHENO_0001001> ;
          rdfs:subClassOf+ [
               owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000051> ;
               owl:someValuesFrom ?trait ] 

 FILTER( !isBlank(?phenotype))
 FILTER( !isBlank(?trait) && STRSTARTS(str(?trait), "http://purl.obolibrary.org/obo/OBA_"))
}